    "# Q. 11286\n",
    "import heapq\n",
    "heap = []\n",
    "        \n",
    "n = int(input())\n",
    "for _ in range(n):\n",
    "    i = int(input())\n",
    "    if i != 0:\n",
    "        heapq.heappush(heap, (abs(i), i))\n",
    "    else:\n",
    "        if len(heap) > 0:\n",
    "            print(heapq.heappop(heap)[1])\n",
    "        else:\n",
    "            print(0)\n",
    "    "
