    "# Q. 2929\n",
    "import re\n",
    "upper_pos = [m.start(0) for m in re.finditer(r'[A-Z]', input())]\n",
    "\n",
    "cnt = 0\n",
    "for i in upper_pos:\n",
    "    if (i + cnt) % 4 != 0:\n",
    "        temp = 0\n",
    "        while (i + cnt + temp) % 4 !=0:\n",
    "            temp += 1\n",
    "        cnt += temp\n",
    "        \n",
    "print(cnt)"
   ]
  }
